{
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "qa": "npm run lint && npm run types && npm run test",
    "test": "jest",
    "types": "flow",
    "lint": "eslint packages/** examples/**",
    "start": "cd examples/react-dragtor-example && yarn start",
    "build:clean": "cd packages/react-dragtor && rimraf ./lib",
    "build:compile": "cd packages/react-dragtor && babel ./src --out-dir ./lib --source-maps --ignore=**/*.test.js",
    "build": "npm run build:clean && npm run build:compile",
    "precommit": "npm run qa",
    "prepush": "npm run qa"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.3.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.21.0",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.79.0",
    "flow-typed": "^2.5.1",
    "husky": "^0.14.3",
    "jest": "20.0.4",
    "lerna": "^3.0.6",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {}
}
